{
  "name": "z5h.graph",
  "version": "0.1.1",
  "description": "a directed graph library",
  "main": "index.js",
  "devDependencies": {
    "nodeunit": "^0.9.0",
    "watchify": "0.9.x",
    "jscoverage": "^0.5.0-rc2",
    "coveralls": "^2.10.0"
  },
  "dependencies": {
